package com.java.practice;

public class variables {
	
	int x;
	static int y;
	
    public static void main(String[] args)
    {
    	variables v = new variables();
    	System.out.println(" x and Y values are "+ y +v.x);
    }
    
    public void display()
    {
    	/* variables v = new variables(); */
    	System.out.println(" x and Y values are "+ y);
    }

}
